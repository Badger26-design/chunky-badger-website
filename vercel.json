{
    "version": 2,
    "builds": [
        {
            "src": "*.{html,css,js,xml,txt}",
            "use": "@vercel/static"
        },
        {
            "src": "Pictures/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "api/*.js",
            "use": "@vercel/node"
        }
    ],
    "headers": [
        {
            "source": "/Pictures/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "routes": [
        {
            "src": "/api/subscribe",
            "dest": "/api/subscribe.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}