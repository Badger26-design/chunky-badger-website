{
    "version": 2,
    "builds": [
        {
            "src": "*.html",
            "use": "@vercel/static"
        },
        {
            "src": "*.css",
            "use": "@vercel/static"
        },
        {
            "src": "*.js",
            "use": "@vercel/static"
        },
        {
            "src": "src/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "Pictures/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "api/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/subscribe",
            "dest": "/api/subscribe.js"
        }
    ]
}