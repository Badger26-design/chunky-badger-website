{
    "headers": [
        {
            "source": "/Pictures/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/api/subscribe",
            "destination": "/api/subscribe.js"
        }
    ]
}